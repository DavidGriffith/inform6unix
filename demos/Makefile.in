install-zcodeDATA: $(zcode_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(zcodedir)
	@list='$(zcode_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(zcodedir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(zcodedir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(zcodedir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(zcodedir)/$$p; \
	  fi; fi; \
	done

